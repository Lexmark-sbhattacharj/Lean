        init: function(options, elem) {
            var self = this;
            options = $.extend( $.fn.be_lexmark.defaults, options);

            // Insert header
            var header = $('header:not([id]):not([class])').first();
            if (header.length > 0) {
                var newheader = $($.parseHTML(this.header_html));
                header.replaceWith(newheader);
                header = newheader;
            } else {
                debug('Inserting new header element found.');
                header = $(this.header_html).prependTo('body');
            }
            // insert footer
            // Wordpress has many footers with id=colophon or class=entry-meta
            var footer = $("footer:not([id]):not([class])").last();
            if (footer.length > 0) {
                var newfooter = $($.parseHTML(this.footer_html));
                footer.replaceWith(newfooter);
                footer = newfooter;
            } else {
                debug('Inserting new footer element.');
                footer = $(this.footer_html).appendTo('body');
            }

            //wrap in div id=wrapper if not already there.
            if ((header.parents('#wrapper').length +
                footer.parents('#wrapper').length) < 2) {
                // andSelf() -> addBack() in JQuery 1.8 and greater.
                header.nextUntil(footer).andSelf().add(footer).wrapAll('<div id="wrapper" />');
            }

            if ($('#logonav-container').length) {
                self.fixedWidthHeader(options);
            } else {
                self.fullWidthHeader(options);
            }
        },

        /**
         * Inserts a javascript file. 
         **/
        addScript: function(url) {
            var s = document.createElement('script');
            s.src = url;
            var h = document.getElementsByTagName('script')[0];
            h.parentNode.insertBefore(s, h);
        },

        /**
         * Customizing the fixed width header.
         **/
        fixedWidthHeader: function(options) {

            this.addScript('//www.lexmark.com/common/js/menu.js');
            $("body").addClass('fixedwidth');
            if (options.search === false) {
                $("#search-ul").remove();
            }
            if (typeof(options.search) === 'function') {
                $("#isys-search-form").submit(function(e) { 
                    e.preventDefault(); 
                    options.search($('#isys-search-form input[type="text"]').val());
                });
                $("#isys-search-button").click(function(e) { 
                    e.preventDefault(); 
                    options.search($('#isys-search-form input[type="text"]').val());
                });
            }
            if (options.signin === false) {
                $("#header-ul").remove();
            }
            if (typeof(options.signin) === 'string') {
                $(options.signin).appendTo('#header-ul').wrap('<li/>');
            }
            var countryLayer = $('#countrylayer'),
                countryLink = $('a.country-link'),
                locale_picker = $("#header-selectbox");
            if (options.pickLocale === false || options.flag === false) {
                locale_picker.remove();  
            }
            if (typeof(options.pickLocale) === 'function') {
                locale_picker.on('click.beLexmark', function(e) {
                    e.preventDefault(); 
                    if (!countryLink.hasClass("active")) {
                        countryLink.addClass('active');
                        countryLayer.slideDown(200);   
                        options.pickLocale(e);
                    } 
                }).on('mouseup.beLexmark', function(e){
                    e.stopPropagation();
                });
                $(document).on('mouseup.beLexmark', function() {
                    if (countryLink.hasClass("active")){
                        countryLink.removeClass('active');
                        countryLayer.hide();
                    }
                });
            }
            if (options.pickLocale === true) {
                this.addScript('//www.lexmark.com/common/js/countryDropdown.js');  
            } 

            if (typeof(options.flag) === "string") {
                var src = $("img.country-flag").attr('src');
                var img = options.flag.toLowerCase().substr(0,2) + ".gif";
                $("img.country-flag").attr('src', src.replace(/[^\/]+$/, img));
            }
            if (options.country === false) {
                $("#header-selectbox .country-label").remove();
            }
            if (typeof(options.country) === "string") {
                $("#header-selectbox .country-label").text(options.country);
            }
            if (options.language === false) {
                $("#country-language").remove();
            }

        },

        /**
         * Customizing the full width header.
         **/
        fullWidthHeader: function(options) {
            if (typeof(window.Modernizr) === 'undefined') {
                debug('Modernizr is not present. Nothing we can do about it now.');
            } else {
                this.addScript('//www.lexmark.com/common/js/srd2013/header.js');  
            }
            $("body").addClass('fullwidth');

            if (options.search === false) {
                $("#logoBarAct form").remove();
            }
            if (typeof(options.search) === 'function') {
                $('#logoBarAct input[type="submit"]').click(function(e) { e.preventDefault(); 
                    options.search($('#logoBarAct input[name="text"]').val());});
            }

            var locale_picker = $('#flagSelect').parent();
            if (typeof(options.signin) === 'string') {
                // insert a custom sign-in link. 
                $(options.signin).insertBefore(locale_picker).wrap('<li/>');
            }

            if (options.flag === false || options.pickLocale === false) {
                // Remove the Country selection.
                locale_picker.prev().addClass('lastNav');
                locale_picker.remove();
            }
            if (typeof(options.pickLocale) === 'function') {
                $('#flagSelect').on('click.beLexmark', function(e){
                    e.preventDefault();
                    options.pickLocale(e);
                }).on('mouseup.beLexmark', function(e){
                    e.stopPropagation();
                });
                $('#countrySelect').on('click.beLexmark', function(e){
                    e.stopPropagation();
                });
            }
            if (typeof(options.flag) === "string") {
                // Replace the country flag.
                var img = options.flag.toLowerCase().substr(0,2) + ".gif";
                $("#flagSelect img").attr('src', 
                    '//www.lexmark.com/common/images/flags/'+ img );
            }
        }

    };
    

    // private function for debugging
    function debug($msg) {
        if (window.console && window.console.log) {
            window.console.log('BeLexmark: ' + $msg);
        }
    }
    
    $.fn.be_lexmark = function(options) {
        var lexmark = Object.create( BeLexmark );
        lexmark.init(options, this);
    };
    
    // Default options
    $.fn.be_lexmark.defaults = {
        search: true,
        flag: true,
        country: true,
        pickLocale: true,
        signin: true,
        language: true,
        version: 9
    };

})(jQuery, window, document);
